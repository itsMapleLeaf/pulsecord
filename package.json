{
  "name": "pulsecord",
  "version": "0.0.0",
  "private": true,
  "main": "build/main.cjs",
  "scripts": {
    "dev": "run-p --race --print-label dev:*",
    "dev:electron": "remix-electron dev",
    "dev:tailwind": "pnpm run tailwind --watch",
    "build": "pnpm run tailwind --minify && remix-electron build",
    "start": "electron .",
    "lint": "eslint --ext js,ts,tsx .",
    "lint-fix": "pnpm lint --fix",
    "format": "prettier --write .",
    "test": "vitest",
    "coverage": "vitest --run --coverage",
    "typecheck": "tsc --noEmit",
    "tailwind": "tailwindcss -c tailwind.config.cjs -o app/tailwind.css"
  },
  "dependencies": {
    "@remix-electron/main": "^2.0.0-alpha.5",
    "@remix-run/node": "^1.4.0",
    "@remix-run/react": "^1.4.0",
    "@remix-run/server-runtime": "^1.4.0",
    "electron": "^18.0.4",
    "pulseaudio.js": "^1.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@itsmapleleaf/configs": "^1.1.3",
    "@remix-electron/cli": "^2.0.0-alpha.5",
    "@remix-electron/renderer": "^2.0.0-alpha.5",
    "@remix-run/dev": "^1.4.0",
    "@rushstack/eslint-patch": "^1.1.3",
    "@types/eslint": "^8.4.2",
    "@types/node": "^17.0.33",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.0",
    "autoprefixer": "^10.4.7",
    "c8": "^7.11.2",
    "eslint": "^8.15.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.13",
    "prettier": "^2.6.2",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.3",
    "vitest": "^0.12.6"
  },
  "prettier": "@itsmapleleaf/configs/prettier"
}
